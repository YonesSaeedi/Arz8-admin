{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "bacon/bacon-qr-code": "^2.0",
        "bepsvpt/secure-headers": "^8.0",
        "beyondcode/laravel-websockets": "^1.12",
        "ezyang/htmlpurifier": "^4.18",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.5",
        "intervention/image": "^2.5",
        "irazasyed/telegram-bot-sdk": "^3.9",
        "jaggedsoft/php-binance-api": "^0.5.28",
        "kavenegar/laravel": "^1.3",
        "kreait/laravel-firebase": "^3.4",
        "kucoin/kucoin-php-sdk": "~1.1.0",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "yones/jalali": "dev-master",
        "pragmarx/google2fa": "^8.0",
        "pragmarx/google2fa-qrcode": "^2.1",
        "predis/predis": "^3.1",
        "pusher/pusher-php-server": "^7.2",
        "spatie/async": "^1.6",
        "spatie/browsershot": "^3.57",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^0.0.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/YonesSaeedi/jalali"
        }
    ]
}
